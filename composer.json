{
  "name": "contestkit/sdk",
  "description": ":package_description",
  "keywords": [
    "ContestKit",
    "laravel",
    "sdk"
  ],
  "homepage": "https://github.com/contestkit/sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaz Jallad",
      "email": "yaz@contestkit.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/laravel-package-tools": "^1.4.3",
    "illuminate/contracts": "^9.19"
  },

  "autoload": {
    "psr-4": {
      "ContestKit\\Sdk\\": "src",
      "ContestKit\\Sdk\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ContestKit\\Sdk\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest --no-coverage",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ContestKit\\Sdk\\ContestKitServiceProvider"
      ],
      "aliases": {
        "ContestKit": "ContestKit\\Sdk\\ContestKit"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
